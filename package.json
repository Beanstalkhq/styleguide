{
  "name": "canopy-styleguide",
  "version": "2.1.0",
  "description": "Style-guide for Canopy",
  "main": "build/styleguide.css",
  "scripts": {
    "setup": "bin/install-git-hooks post-merge",
    "clean": "rm build/*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack.config.build.js",
    "deploy": "cp build/* ../workflow-ui/node_modules/canopy-styleguide/build/",
    "prepublish": "npm run build",
    "patch-canopy": "echo 'Please ensure that your git working directory is clean' && npm version patch -m 'npm run patch-canopy has incremented the patch version of canopy-styleguide to %s' && npm publish && cd ../canopy && npm install && npm install canopy-styleguide@latest --save --save-exact && npm shrinkwrap && echo 'Please make sure to commit and push any changed built files to source control'"
  },
  "git-tasker": [
    {
      "matcher": "package.json",
      "command": "npm install"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/canopytax/styleguide.git"
  },
  "author": "Bret Little",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/canopytax/styleguide/issues"
  },
  "homepage": "https://github.com/canopytax/styleguide",
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "css-loader": "^0.9.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "git-tasker": "^1.0.1",
    "html-loader": "^0.2.3",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.12.2",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "lodash": "^3.5.0",
    "react": "^0.12.2",
    "react-router": "^0.12.4",
    "style-loader": "^0.8.3",
    "webpack": "^1.7.2"
  },
  "dependencies": {
    "canopy-iconography": "1.5.1",
    "file-loader": "^0.8.4",
    "highlight.js": "^8.4.0",
    "react-highlight": "^0.4.1",
    "url-loader": "^0.5.6"
  },
  "publishConfig": {
    "registry": "http://npm.canopytax.com"
  }
}
